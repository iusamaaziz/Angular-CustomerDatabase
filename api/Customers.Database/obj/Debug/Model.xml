		<Element Type="SqlProcedure" Name="[dbo].[UpsertPhoneNumber]">
			<Property Name="BodyScript">
				<Value><![CDATA[
	
	IF (@id = 0)
	BEGIN
		INSERT INTO dbo.PhoneNumbers([Number], [AddressId])
		VALUES (@number, @address);
	END
	ELSE
	BEGIN
		UPDATE dbo.PhoneNumbers
		SET
			[Number] = @number
		where [Id] = @id;

	END]]></Value>
